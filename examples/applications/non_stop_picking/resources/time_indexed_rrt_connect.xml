<?xml version="1.0" ?>
<PlannerDemoConfig>

  <TimeIndexedRRTConnect Name="TimeIndexedRRTConnect">
    <Timeout>5</Timeout>
    <AddTimeIntoSolution>1</AddTimeIntoSolution>
  </TimeIndexedRRTConnect>

  <TimeIndexedSamplingProblem Name="TimeIndexedSamplingProblem">
    <T>10</T>
    <GoalTime>10</GoalTime>
    <PlanningScene>
      <Scene>
        <JointGroup>arm</JointGroup>
        <LoadScene>{non_stop_picking}/resources/demo.scene</LoadScene>
        <Trajectories>
          <Trajectory Link="Obstacle" File="{non_stop_picking}/resources/moving_obstacle.traj" />
          <Trajectory Link="Table" File="{non_stop_picking}/resources/moving_table.traj" />
        </Trajectories>
        <CollisionScene>CollisionSceneFCLLatest</CollisionScene>
        <AlwaysUpdateCollisionScene>0</AlwaysUpdateCollisionScene>
      </Scene>
    </PlanningScene>

    <Maps>
      <CollisionCheck Name="Collision" SelfCollision="1" />
    </Maps>

    <Constraint>
      <Task Task="Collision"/>
    </Constraint>

    <JointVelocityLimits>1 1 1 1 1 1 1</JointVelocityLimits>
    <!--Goal>2.16939  1.313509   -2.2954   1.94413 -0.276843  0.567194         0</Goal-->
    <Goal>0 0 0 0 0 0 0</Goal>
  </TimeIndexedSamplingProblem>

</PlannerDemoConfig>
