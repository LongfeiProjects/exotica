<?xml version="1.0" ?>
<IKSolverDemoConfig>

  <IKsolver Name="MySolver">
    <MaxIt>100</MaxIt>
    <MaxStep>0.1</MaxStep>
    <Tolerance>1e-5</Tolerance>
    <Alpha>1.0</Alpha>
    <C>1e-3</C>
  </IKsolver>

  <UnconstrainedEndPoseProblem Name="MyProblem">

    <PlanningScene>
      <Scene>
        <JointGroup>arm</JointGroup>
        <LoadScene>{non_stop_picking}/resources/demo.scene</LoadScene>
        <Trajectories>
          <Trajectory Link="Obstacle" File="{non_stop_picking}/resources/moving_obstacle.traj" />
          <Trajectory Link="Table" File="{non_stop_picking}/resources/moving_table.traj" />
        </Trajectories>
      </Scene>
    </PlanningScene>
    
    <Maps>
      <EffFrame Name="Position">
        <EndEffector>
            <Frame Link="lwr_arm_6_link" LinkOffset="0 0 0 0.7071067811865476 -4.3297802811774664e-17  0.7071067811865475 4.3297802811774664e-17"/>
        </EndEffector>
      </EffFrame>
    </Maps>

    <Cost>
      <Task Task="Position"/>
    </Cost>

    <StartState>0 0 0 0 0 0 0</StartState>
    <NominalState>0 0 0 0 0 0 0</NominalState>
    <W> 7 6 5 4 3 2 1 </W>
  </UnconstrainedEndPoseProblem>

</IKSolverDemoConfig>
