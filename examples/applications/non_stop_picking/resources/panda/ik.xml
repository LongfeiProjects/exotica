<?xml version="1.0" ?>
<IKSolverDemoConfig>

  <IKsolver Name="MySolver">
    <MaxIt>100</MaxIt>
    <MaxStep>0.1</MaxStep>
    <Tolerance>1e-5</Tolerance>
    <Alpha>1.0</Alpha>
    <C>1e-3</C>
  </IKsolver>

  <UnconstrainedEndPoseProblem Name="MyProblem">

    <PlanningScene>
      <Scene>
        <JointGroup>arm</JointGroup>
        <LoadScene>{non_stop_picking}/resources/panda/demo.scene</LoadScene>
        <Trajectories>
          <Trajectory Link="Obstacle" File="{non_stop_picking}/resources/panda/moving_obstacle.traj" />
          <Trajectory Link="Table" File="{non_stop_picking}/resources/panda/moving_table.traj" />
        </Trajectories>
      </Scene>
    </PlanningScene>
    
    <Maps>
      <EffPosition Name="Position">
        <EndEffector>
            <Frame Link="panda_hand"/>
        </EndEffector>
      </EffPosition>
      <EffOrientation Name="Orientation">
        <EndEffector>
            <Frame Link="panda_hand" LinkOffset="0 0 0 1.57 -1.57 1.57"/>
        </EndEffector>
      </EffOrientation>
      <JointLimit Name="JointLimit">
        <SafePercentage>0.1</SafePercentage>
      </JointLimit>
    </Maps>

    <Cost>
      <Task Task="Position"/>
      <Task Task="Orientation"/>
      <Task Task="JointLimit"/>
    </Cost>

    <StartState>0 0 0 0 0 0 0</StartState>
    <NominalState>0 0 0 0 0 0 0</NominalState>
    <W> 7 6 5 4 3 2 1 </W>
  </UnconstrainedEndPoseProblem>

</IKSolverDemoConfig>
