<?xml version="1.0" ?>
<PlannerDemoConfig>

  <TimeIndexedRRTConnect Name="TimeIndexedRRTConnect">
    <Timeout>5</Timeout>
    <AddTimeIntoSolution>1</AddTimeIntoSolution>
  </TimeIndexedRRTConnect>

  <TimeIndexedSamplingProblem Name="TimeIndexedSamplingProblem">
    <T>10</T>
    <GoalTime>10</GoalTime>
    <PlanningScene>
      <Scene>
        <JointGroup>arm</JointGroup>
        <LoadScene>{non_stop_picking}/resources/panda/box/demo.scene</LoadScene>
        <Trajectories>
          <Trajectory Link="Table" File="{non_stop_picking}/resources/panda/box/moving_box.traj" />
        </Trajectories>
        <CollisionScene>CollisionSceneFCL</CollisionScene>
        <AlwaysUpdateCollisionScene>0</AlwaysUpdateCollisionScene>
      </Scene>
    </PlanningScene>

    <Maps>
      <CollisionCheck Name="Collision" SelfCollision="1" />
    </Maps>

    <Constraint>
      <Task Task="Collision"/>
    </Constraint>

    <StartState>0 -0.5657 0 -2.4297 0 3.4346 0.7522</StartState>
    <JointVelocityLimits>1 1 1 1 1 1 1</JointVelocityLimits>
    <Goal>1.7 -0.5657 0 -2.4297 0 3.4346 0.7522</Goal>
  </TimeIndexedSamplingProblem>

</PlannerDemoConfig>
