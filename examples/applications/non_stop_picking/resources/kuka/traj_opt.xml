<?xml version="1.0" ?>
<IKSolverDemoConfig>
  <OptppTrajLBFGS Name="MySolver">
    
  </OptppTrajLBFGS>

  <UnconstrainedTimeIndexedProblem Name="MyProblem">

    <PlanningScene>
      <Scene>
        <JointGroup>arm</JointGroup>
        <LoadScene>{non_stop_picking}/resources/demo.scene</LoadScene>
        <Trajectories>
          <Trajectory Link="Target" File="{non_stop_picking}/resources/constraint.traj" />
        </Trajectories>
      </Scene>
    </PlanningScene>
    
    <Maps>
      <EffPosition Name="Position">
        <EndEffector>
            <Frame Link="lwr_arm_6_link" Base="Target"/>
        </EndEffector>
      </EffPosition>
      <JointLimit Name="JointLimit">
        <SafePercentage>0.1</SafePercentage>
      </JointLimit>
    </Maps>

    <Cost>
      <Task Task="Position"/>
      <Task Task="JointLimit"/>
    </Cost>

    <StartState>2.61396  -1.44751   1.16127  -2.68349   -0.6147 0.0952065  -2.54523</StartState>
    <!-- <NominalState>0 0 0 0 0 0 0</NominalState> -->
    <T>100</T>
    <Tau>0.03</Tau>
    <Qrate>1e-10</Qrate>
    <Hrate>1</Hrate>
    <Wrate>1e3</Wrate>
    <W> 1 1 1 1 1 1 1 </W>
  </UnconstrainedTimeIndexedProblem>

</IKSolverDemoConfig>
